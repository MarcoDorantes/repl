<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>nutility</RootNamespace>
    <PackageId>nRepl</PackageId>
    <Authors>Marco A. Dorantes</Authors>
    <Product>nRepl</Product>
    <Description>A basic library to support Read-Evaluate-Print-Loops (REPL).</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/marcodorantes/repl</RepositoryUrl>
    <PackageProjectUrl>https://github.com/marcodorantes/repl</PackageProjectUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>CLI REPL</PackageTags>
    <PackageReleaseNotes>Initial release.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="nnTree" Version="1.0.1" />
    <PackageReference Include="nSwitch" Version="3.7.0" />
  </ItemGroup>

</Project>